{
    "name": "iget-master/cielo-checkout",
    "description": "Provides simple Cielo Checkout integration on Laravel 5",
    "require": {
        "illuminate/support": "^6.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "laravel/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Iget\\CieloCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Elias Soares",
            "email": "esoares@iget.com.br"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Iget\\CieloCheckout\\ServiceProvider"
            ],
            "aliases": {
                "Cielo": "Iget\\CieloCheckout\\CieloFacade"
            }
        }
    }
}
