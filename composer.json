{
    "name": "iget-master/cielo-checkout",
    "description": "Provides simple Cielo Checkout integration on Laravel 5",
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "laravel/laravel": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x"
    },
    "autoload": {
        "psr-4": {
            "Iget\\CieloCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Elias Soares",
            "email": "esoares@iget.com.br"
        }
    ],
    "minimum-stability": "stable"
}
