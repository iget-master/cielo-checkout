{
    "name": "iget-master/cielo-checkout",
    "description": "Provides simple Cielo Checkout integration on Laravel 5",
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.x",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "phpspec/phpspec": "~2.1",
        "laravel/framework": "5.x"
    },
    "autoload": {
        "psr-4": {
            "Iget\\CieloCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Elias Soares",
            "email": "esoares@iget.com.br"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Iget\\CieloCheckout\\ServiceProvider"
            ],
            "aliases": {
                "Cielo": "Iget\\CieloCheckout\\CieloFacade"
            }
        }
    }
}
